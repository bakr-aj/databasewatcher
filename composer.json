{
    "name": "bakraj/databasewatcher",
    "description": "Laravel Package to track and monitor DB requests",
    "license": "MIT",
    "authors": [
        {
            "name": "bakr aj",
            "email": "bakr.aj@tssarmada.com",
            "homepage": "tssarmada.com"
        }
    ],
    "homepage": "https://github.com/bakraj/databasewatcher",
    "keywords": ["Laravel", "DataBaseWatcher","database","monitoring"],
    "require": {
        "illuminate/support": "~5",
        "monolog/monolog": "~1.12",
        "league/flysystem": "^1.0.8"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0",
        "orchestra/testbench": "~3.0"
    },
    "autoload": {
        "psr-4": {
            "bakraj\\DataBaseWatcher\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "bakraj\\DataBaseWatcher\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "bakraj\\DataBaseWatcher\\DataBaseWatcherServiceProvider"
            ],
            "aliases": {
                "DataBaseWatcher": "bakraj\\DataBaseWatcher\\Facades\\DataBaseWatcher"
            }
        }
    }
}